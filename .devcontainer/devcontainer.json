{
  "name": "golang-app",
  "dockerComposeFile": ["../docker-compose.yml"],
  "service": "go-app",
  "customizations": {
    "vscode": {
      "extensions": ["golang.go"],
      "settings": {
        "files.encoding": "utf8",
        "files.eol": "\n",
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true,
        "[go]": {
          "editor.defaultFormatter": "golang.go"
        }
      }
    }
  },
  "forwardPorts": [8000]
}
